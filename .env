# ROS2 环境变量配置
ROS_DISTRO=humble
ROS_DOMAIN_ID=0
RMW_IMPLEMENTATION=rmw_cyclonedx_cpp

# ByteTracker 调试选项
BYTETRACKER_DEBUG_MODE=1
BYTETRACKER_VERBOSE=1
BYTETRACKER_LOG_LEVEL=DEBUG

# 可选：禁用相机用于调试（如果没有摄像头）
# BYTETRACKER_NO_CAMERA=1

# 可选：使用模拟数据进行调试
# BYTETRACKER_USE_MOCK_DATA=1

# OpenCV 调试选项
OPENCV_LOG_LEVEL=ERROR

# Python 调试选项
PYTHONUNBUFFERED=1
PYTHONDONTWRITEBYTECODE=1
